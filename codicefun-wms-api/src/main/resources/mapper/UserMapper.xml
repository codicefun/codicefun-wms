<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.codicefun.wms.mapper.UserMapper">

  <select id="selectByAge" resultType="com.codicefun.wms.entity.po.User">
    SELECT *
    FROM `user`
    WHERE `age` = #{age}
  </select>

</mapper>
